n    const url = `${API}/user/photo/${username}`\r\n    const [{\r\n        srcBlob,\r\n        srcDataUri\r\n    }, setSrc] = useState({\r\n        srcBlob: null,\r\n        srcDataUri: null\r\n    });\r\n\r\n    useEffect(() => {\r\n        let isUnmounted = false;\r\n\r\n        fetch(url, {})\r\n            .then(response => response.blob())\r\n            .then(blob => blob.arrayBuffer())\r\n            .then(arrayBuffer => {\r\n\r\n                if (isUnmounted) {\r\n                    return;\r\n                }\r\n\r\n                const blob = new Blob([arrayBuffer])\r\n                const srcBlob = URL.createObjectURL(blob);\r\n                const slicedBlob = srcBlob.slice(5)\r\n                setSrc(state => ({\r\n                    ...state,\r\n                    slicedBlob\r\n                }));\r\n\r\n            })\r\n\r\n        return () => {\r\n            isUnmounted = true;\r\n        }\r\n\r\n    }, [])\r\n\r\n    const init = () => {\r\n        getProfile(token).then(profile => {\r\n            if (profile.error || undefined) {\r\n                setValues({...values, error: profile.error ?? undefined})\r\n            } else {\r\n                setValues({\r\n                    ...values,\r\n                    name: profile.name,\r\n                    username: profile.username,\r\n                    senderEmail: profile.email,\r\n                    about: profile.about,\r\n                })\r\n            }\r\n        })\r\n    }\r\n\r\n\r\n    const handleChange = name => e => {\r\n        // console.log(e.target.value);\r\n        const value = name === 'photo' ? e.target.files[0] : e.target.value;\r\n        let userFormData = new FormData()\r\n        userFormData.set(name, value);\r\n        setValues({...values, [name]: value, userData: userFormData, error: '', success: false});\r\n    };\r\n\r\n    const handleSubmit = (e) => {\r\n        e.preventDefault()\r\n        setValues({...values, loading: true});\r\n        updateProfile(userData, token).then((data) => {\r\n            if (data.error) {\r\n                setValues({...values, error: data.error, success: false, loading: false});\r\n            } else {\r\n\r\n                update(data, () => {\r\n                    setValues({\r\n                        ...values,\r\n                        name: data.name,\r\n                        username: data.username,\r\n                        senderEmail: data.email,\r\n                        about: data.about,\r\n                        photo: data.photo,\r\n                        success: true,\r\n                        loading: false\r\n                    })\r\n                })\r\n            }\r\n        })\r\n    }\r\n\r\n    useEffect(() => {\r\n        init()\r\n    }, [])\r\n\r\n\r\n    const updateForm = () => (\r\n        <form onSubmit={handleSubmit}>\r\n            <div className=\"form-group\">\r\n                <label className=\"btn btn-outline-info\">\r\n                    Profile photo\r\n                    <input type=\"file\" accept=\"image/*\" hidden className=\"form-control\"\r\n                           onChange={handleChange('photo')}/>\r\n                </label>\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Name</label>\r\n                <input type=\"text\" className=\"form-control\" value={name} onChange={handleChange('name')}/>\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Password</label>\r\n                <input type=\"password\" className=\"form-control\" value={password} onChange={handleChange('password')}/>\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Email</label>\r\n                <input type=\"email\" className=\"form-control\" value={email} onChange={handleChange('email')}/>\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">About</label>\r\n                <input type=\"textarea\" className=\"form-control\" value={about} onChange={handleChange('about')}/>\r\n            </div>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Username</label>\r\n                <input type=\"text\" className=\"form-control\" value={username} onChange={handleChange('username')}/>\r\n            </div>\r\n            <div>\r\n                <button type=\"submit\" className=\"btn btn-primary\">Submit</button>\r\n            </div>\r\n        </form>\r\n    )\r\n\r\n    const showError = () => (\r\n        <div className=\"alert alert-danger\" style={{display: error ? '' : 'none'}}>\r\n            {error}\r\n        </div>\r\n    );\r\n\r\n    const showSuccess = () => (\r\n        <div className=\"alert alert-success\" style={{display: success ? '' : 'none'}}>\r\n            Profile was successfully updated\r\n        </div>\r\n    );\r\n\r\n    const showLoading = () => (\r\n        <div className=\"alert alert-success\" style={{display: loading ? '' : 'none'}}>\r\n            Loading.....\r\n        </div>\r\n    );\r\n\r\n    console.log(srcBlob)\r\n\r\n    return (\r\n        <>\r\n            <div className=\"container\">\r\n                <div className=\"row\">\r\n                    <div className=\"col-md-4 mb-5\">\r\n                        {srcBlob ? <img src={srcBlob} className=\"img img-fluid mb-3\"/> : \"🤷 Can't find the user image\"}\r\n                    </div>\r\n                    <div className=\"col-md-8\">\r\n                        {showError()}\r\n                        {showLoading()}\r\n                        {showSuccess()}\r\n                        update form\r\n                        <hr/>\r\n                        {updateForm()}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </>\r\n    );\r\n};\r\n\r\nexport default ProfileUpdate;\r\n"]},"metadata":{},"sourceType":"module"}